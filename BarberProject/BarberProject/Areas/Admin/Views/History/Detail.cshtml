@model HistoryDetailVM

@{
    ViewData["Title"] = "Detail";
}

<div class="container">


    <div class="table-responsive table--no-card m-b-30">
        <table class="table table-borderless table-striped table-earning">
            <thead>
                <tr>
                    <th>UpTitle</th>
                    <th>MainTitle</th>
                    <th>Description</th>
                    <th>Image</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>@Model.UpTitle</td>
                    <td>@Model.MainTitle</td>
                    <td>@Model.Description</td>
                    <td><img src="~/images/@Model.Image" style="width:200px" /></td>
                </tr>
            </tbody>
        </table>
    </div>

    <a asp-action="Index" asp-controller="History" class="btn btn-danger">Back</a>

</div>

